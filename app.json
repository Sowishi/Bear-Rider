{
  "expo": {
    "name": "Bear Rider Express",
    "slug": "super-laco",
    "version": "2.0.10",
    "orientation": "portrait",
    "icon": "./assets/bear.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "splash": {
      "image": "./assets/bear.png",
      "resizeMode": "contain",
      "backgroundColor": "#B80B00"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to access camera.",
        "NSMicrophoneUsageDescription": "Allow $(PRODUCT_NAME) to access your microphone"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/bear.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "FOREGROUND_SERVICE"
      ],
      "package": "com.bearrider",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDJ92GRaQrePL4SXQEXF0qNVdAsbVhseYI"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "7ce5edba-c433-42ea-85e8-4689f8a421ea"
      }
    },
    "owner": "sowishi"
  }
}
